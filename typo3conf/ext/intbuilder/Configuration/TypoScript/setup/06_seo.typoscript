# ----------------------------------------------------------------------------------------------------------------------
# EXT:seo Sitemap Settings
# ----------------------------------------------------------------------------------------------------------------------

plugin.tx_seo {
    config {
        xmlSitemap {
            sitemaps {
                pages {
                    config {
                        excludedDoktypes = 137, 138
                        additionalWhere = AND no_index = 0 AND canonical_link = ''
                    }
                }
            }
        }
    }
}