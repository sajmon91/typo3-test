        # cat=plugin.tx_test_newslist/file; type=string; label=Path to template partials (FE)
        partialRootPath = EXT:test/Resources/Private/Partials/
        # cat=plugin.tx_test_newslist/file; type=string; label=Path to template layouts (FE)
        layoutRootPath = EXT:test/Resources/Private/Layouts/
    }
    persistence {
        # cat=plugin.tx_test_newslist//a; type=string; label=Default storage PID
